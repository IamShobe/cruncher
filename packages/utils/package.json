{
  "name": "@cruncher/utils",
  "version": "0.2.2",
  "description": "Utility functions for Cruncher.",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cruncher",
    "qql",
    "query-language",
    "coralogix",
    "adapter"
  ],
  "author": {
    "name": "Elran Shefer"
  },
  "license": "GPL-3.0-only",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
