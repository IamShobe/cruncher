{
  "name": "cruncher-monorepo",
  "private": true,
  "version": "1.5.0",
  "packageManager": "pnpm@10.12.1",
  "description": "Cruncher monorepo.",
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "sharp"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IamShobe/cruncher.git"
  },
  "scripts": {
    "start": "pnpm run -F @cruncher/app start",
    "start:docs": "pnpm run -F ./docs start",
    "build": "pnpm run --recursive --filter \"./packages/**\" build",
    "publish": "pnpm run build && pnpm run -F @cruncher/app publish",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "keywords": [],
  "author": {
    "name": "Elran Shefer"
  },
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@nx/js": "^21.2.1",
    "@nx/vite": "21.2.1",
    "@nx/web": "21.2.1",
    "@release-it/bumper": "^7.0.5",
    "@vitest/ui": "^3.0.0",
    "jiti": "2.4.2",
    "nx": "21.2.1",
    "prettier": "^3.5.3",
    "release-it": "^19.0.3",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "nx": {},
  "dependencies": {}
}
